<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0" default-access="nosetter.camelcase-underscore" default-lazy="true">
  <class name="Entities.Contacts, Entities" table="[Contacts]" dynamic-update="true">    
		<id name="Id" column="CON_ID"><generator class="guid.comb" /></id>
	  <property name="Name" column="CON_NAME"/>
	  <property name="Address" column="CON_ADDRESS"/>
	  <property name="Code" column="CON_CODE"/>
	  <property name="Phone" column="CON_PHONE"/>
	  <property name="Fax" column="CON_FAX"/>
	  <property name="Mail" column="CON_MAIL"/>
	  <property name="Bank" column="CON_BANK"/>
	  <property name="Swift" column="CON_SWIFT"/>
	  <property name="Account" column="CON_ACCOUNT"/>
  </class>
</hibernate-mapping>
