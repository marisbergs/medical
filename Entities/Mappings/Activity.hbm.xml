<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0" default-access="nosetter.camelcase-underscore" default-lazy="true">
  <class name="Entities.Activity, Entities" table="[Activities]" dynamic-update="true">
    
    <id name="Id" column="UAC_ID"><generator class="guid.comb" /></id>	  
    <property name="DeleteTime" column="UAC__DELETE_TIME" />
    <property name ="Patient" column ="UAC_PATIENT_ID"/>
    <property name ="User" column ="UAC_USER_ID"/>
    <property name ="Service" column ="UAC_SERVICE_ID"/>
    <property name ="Revactinacion" column ="UAC_REVACTINACION"/>
    <property name ="ActivityDate" column ="UAC_DATE"/>
    <property name ="Comments" column ="UAC_COMMENTS"/>
    <property name ="CreateUser" column ="UAC_CREATE_USER"/>
    <property name ="CreateDate" column ="UAC_CRAEATE_DATE"/>
    <property name ="RemindDate" column ="UAC_REMIND_DATE"/>
    <property name ="Group" column ="UAC_GROUP_ID"/>
    <property name ="Visit" column ="UAC_VISIT_ID"/>
    <property name ="SendSMS" column ="UAC_SEND_SMS"/>
  </class>
</hibernate-mapping>
